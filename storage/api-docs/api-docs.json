        "/api/social-login": {
            "post": {
                "tags": [
                    "Auth Management"
                ],
                "summary": "Social login",
                "description": "Social login here",
                "operationId": "Social Login",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "provider",
                                    "provider_id",
                                    "subscription_id"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "text"
                                    },
                                    "email": {
                                        "type": "text"
                                    },
                                    "phone": {
                                        "type": "integer"
                                    },
                                    "avatar": {
                                        "type": "file"
                                    },
                                    "provider": {
                                        "type": "text"
                                    },
                                    "provider_id": {
                                        "type": "text"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {}
                        }
                    }
