{
    "openapi": "3.0.0",
    "info": {
        "title": "Laravel",
        "description": "https://laravel/api",
        "version": "1.0.0"
    },
    "paths": {
        "/api/social-login": {
            "post": {
                "tags": [
                    "Auth Management"
                ],
                "summary": "User Social Login",
                "description": "User Social Login here",
                "operationId": "Social Login",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "provider",
                                    "provider_id",
                                    "device_type",
                                    "device_token"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "text"
                                    },
                                    "provider": {
                                        "type": "text",
                                        "enum": [
                                            "google",
                                            "facebook",
                                            "instagram"
                                        ]
                                    },
                                    "email": {
                                        "type": "text"
                                    },
                                    "provider_id": {
                                        "type": "text"
                                    },
                                    "device_type": {
                                        "type": "text",
                                        "enum": [
                                            "ios",
                                            "android"
                                        ]
                                    },
                                    "device_token": {
                                        "type": "text"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Login successfully.",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "404": {
                        "description": "Login failed!"
                    }
                }
            }
        },
        "/api/register": {
            "post": {
                "tags": [
                    "Auth Management"
                ],
                "summary": "User Register",
                "description": "User Register here",
                "operationId": "Register",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "phone",
                                    "location",
                                    "user_type",
                                    "password",
                                    "confirm_password"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "text"
                                    },
                                    "email": {
                                        "description": "optional",
                                        "type": "string"
                                    },
                                    "phone": {
                                        "type": "integer"
                                    },
                                    "location": {
                                        "type": "text"
                                    },
                                    "user_type": {
                                        "type": "text",
                                        "enum": [
                                            "customer",
                                            "artist"
                                        ]
                                    },
                                    "password": {
                                        "type": "password"
                                    },
                                    "confirm_password": {
                                        "type": "password"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Register Successfully",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "404": {
                        "description": "Resource Not Found"
                    }
                }
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "Auth Management"
                ],
                "summary": "User Login",
                "description": "Login User Here",
                "operationId": "authLogin",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "description": "email or phone",
                                        "type": "text"
                                    },
                                    "password": {
                                        "type": "password"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login Successfully",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "404": {
                        "description": "Resource Not Found"
                    }
                }
            }
        },
        "/api/user": {
            "get": {
                "tags": [
                    "User Details"
                ],
                "summary": "User Details Fetch",
                "description": "Get User Details ",
                "operationId": "User Details",
                "responses": {
                    "201": {
                        "description": "Register Successfully",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "404": {
                        "description": "Resource Not Found"
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/otp-verify": {
            "post": {
                "tags": [
                    "Auth Management"
                ],
                "summary": "User Otp Verify",
                "description": "User Otp Verify here",
                "operationId": "Otp Verify",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "otp",
                                    "user_id"
                                ],
                                "properties": {
                                    "otp": {
                                        "type": "text"
                                    },
                                    "user_id": {
                                        "type": "number"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Otp verify successfully.",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "404": {
                        "description": "'otp invalid!"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "sanctum": {
                "type": "http",
                "description": "Laravel Sanctum token authentication",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }
}